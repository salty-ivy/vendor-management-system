# django ENGINE
ENGINE=django.db.backends.postgresql
# or django.db.backends.sqlite3

# database name
DB_NAME=db.sqlite3
# or any other valid name

# database admin user on the database server
SQL_USER=postgres

# credential password for the database admin user
SQL_PASSWORD=someexamplepassword

# host name for DB
DB_HOST=db

# port for database server
DB_PORT=5432
